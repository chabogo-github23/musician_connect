{
  "builds": [
    {
      "src": "musician_connect/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "includeFiles": [
          "static/**",
          "templates/**",
          "*.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/admin",
      "dest": "musician_connect/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "musician_connect/wsgi.py"
    }
  ],
  "hooks": {
    "postDeploy": [
      "python manage.py collectstatic --noinput",
      "python manage.py migrate --noinput"
    ]
  },
  "env": {
    "DEBUG": "False",
    "DISABLE_COLLECTSTATIC": "0"
  }
}